        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="84dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/linear_data_penjaga_lahan"
                android:layout_width="wrap_content"
                android:layout_height="27dp"
                android:layout_centerInParent="true"
                android:layout_gravity="center_horizontal"
                android:layout_marginEnd="8dp"
                android:layout_marginStart="8dp"
                android:fontFamily="@font/raleway_regular"
                android:text="Data Penjaga Lahan"
                android:textColor="@color/md_red_A600"
                android:textSize="19sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.503"
                app:layout_constraintStart_toStartOf="parent" />
        </android.support.constraint.ConstraintLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/md_grey_f3"
                    android:paddingLeft="12dp"
                    android:paddingRight="12dp">
                    <TextView
                        android:text="Nama *"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/segoeui"
                        android:textColor="@color/md_grey_700"
                        android:textSize="13sp"
                        android:textStyle="bold"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/md_grey_f3"
                    android:paddingLeft="12dp"
                    android:paddingRight="12dp">

                    <EditText
                        android:id="@+id/nama_penjaga_lahan"
                        android:layout_width="0dp"
                        android:layout_height="25dp"
                        android:layout_weight="70"
                        android:background="@drawable/edittext_bg"
                        android:text=""
                        android:textSize="13sp"></EditText>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/md_grey_f3"
                    android:paddingLeft="12dp"
                    android:paddingRight="12dp">


                    <TextView
                        android:text="Alamat *"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/segoeui"
                        android:textColor="@color/md_grey_700"
                        android:textSize="13sp"
                        android:textStyle="bold" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/md_grey_f3"
                    android:paddingLeft="12dp"
                    android:paddingRight="12dp">

                    <EditText
                        android:id="@+id/alamat_penjaga_lahan"
                        android:layout_width="0dp"
                        android:layout_height="25dp"
                        android:layout_weight="70"
                        android:background="@drawable/edittext_bg"
                        android:breakStrategy="balanced"
                        android:fitsSystemWindows="true"
                        android:inputType="textMultiLine"
                        android:singleLine="false"
                        android:textSize="13sp" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/md_grey_f3"
                    android:paddingLeft="12dp"
                    android:paddingRight="12dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/segoeui"
                        android:text="Provinsi *"
                        android:textColor="@color/md_grey_700"
                        android:textSize="13sp"
                        android:textStyle="bold" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/md_grey_f3"
                    android:paddingLeft="12dp"
                    android:paddingRight="12dp">

                    <EditText
                        android:id="@+id/provinsi_penjaga_lahan"
                        android:layout_width="0dp"
                        android:layout_height="25dp"
                        android:layout_weight="70"
                        android:background="@drawable/edittext_bg"
                        android:text=""
                        android:textSize="13sp"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/md_grey_f3"
                    android:paddingLeft="12dp"
                    android:paddingRight="12dp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/segoeui"
                        android:text="Kabupaten *"
                        android:textColor="@color/md_grey_700"
                        android:textSize="13sp"
                        android:textStyle="bold" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/md_grey_f3"
                    android:paddingLeft="12dp"
                    android:paddingRight="12dp">

                    <EditText
                        android:id="@+id/kabupaten_penjaga_lahan"
                        android:layout_width="0dp"
                        android:layout_height="25dp"
                        android:layout_weight="70"
                        android:background="@drawable/edittext_bg"
                        android:text=""
                        android:textSize="13sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/md_grey_f3"
                    android:paddingLeft="12dp"
                    android:paddingRight="12dp">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="30"
                        android:text="Kecamatan *"
                        android:textSize="13sp"
                        android:textStyle="bold" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/md_grey_f3"
                    android:paddingLeft="12dp"
                    android:paddingRight="12dp">

                    <EditText
                        android:id="@+id/kecamatan_penjaga_lahan"
                        android:layout_width="0dp"
                        android:layout_height="25dp"
                        android:layout_weight="70"
                        android:background="@drawable/edittext_bg"
                        android:text=""
                        android:textSize="13sp" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/md_grey_f3"
                    android:paddingLeft="12dp"
                    android:paddingRight="12dp">


                    <TextView
                        android:text="No TLP *"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/segoeui"
                        android:textColor="@color/md_grey_700"
                        android:textSize="13sp"
                        android:textStyle="bold" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/md_grey_f3"
                    android:paddingLeft="12dp"
                    android:paddingRight="12dp">

                    <EditText
                        android:id="@+id/no_phone_penjaga_lahan"
                        android:layout_width="0dp"
                        android:layout_height="25dp"
                        android:layout_weight="70"
                        android:background="@drawable/edittext_bg"
                        android:text=""
                        android:textSize="13sp"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/md_grey_f3"
                    android:paddingLeft="12dp"
                    android:paddingRight="12dp">


                    <TextView
                        android:text="No KTP *"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/segoeui"
                        android:textColor="@color/md_grey_700"
                        android:textSize="13sp"
                        android:textStyle="bold"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/md_grey_f3"
                    android:paddingLeft="12dp"
                    android:paddingRight="12dp">


                    <EditText
                        android:id="@+id/no_ktp_penjaga_lahan"
                        android:layout_width="0dp"
                        android:layout_height="25dp"
                        android:layout_weight="70"
                        android:background="@drawable/edittext_bg"
                        android:text=""
                        android:textSize="13sp" />
                </LinearLayout>

            </LinearLayout>

