    android:background="@color/md_grey_f3"
    android:orientation="vertical"
    tools:context="com.cudo.mproject.Menu.Site.Person.PersonActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="8dp">

